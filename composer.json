{
  "name": "lacore-payment-tech/lacore-php",
  "version": "1.0.1",
  "description": "A PHP HTTP Client conforming to the HAL hypermedia type for the Lacore processing API",
  "license": "Apache2",
  "authors": [
    {
      "name": "Lacore",
      "email": "support@lacorepayments.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "finix/processing-php": "==1.0.5",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-0": {
      "Lacore\\": "./src"
    },
    "psr-4": {
      "Lacore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lacore\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
